awk '{lines[NR] = $0; codes[NR] = $1} 
END {
    for (i = 1; i <= NR; i++)
        for (j = i + 1; j <= NR; j++)
            if (codes[i] > codes[j]) {
                temp = lines[i]; lines[i] = lines[j]; lines[j] = temp;
                temp_code = codes[i]; codes[i] = codes[j]; codes[j] = temp_code;
            }
    for (i = 1; i <= NR; i++) print lines[i]
}' clientes

